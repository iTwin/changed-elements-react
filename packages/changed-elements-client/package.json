  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "tsc --project ./tsconfig.build.json --outDir ./lib/cjs --module CommonJS",
