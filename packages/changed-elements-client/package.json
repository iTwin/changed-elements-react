    "test:cover": "vitest run --coverage",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "@vitest/coverage-c8": "^0.24.3",
    "happy-dom": "^7.6.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  }
