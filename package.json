      "@itwin/core-quantity": "^5.1.7",
      "@itwin/core-react": "^5.15.0",
      "axios@<1.8.2": ">=1.8.2",
      "dompurify@<3.2.4": ">=3.2.4",
      "esbuild@<=0.24.2": ">=0.25.0",
      "eslint-plugin-prettier": ">=5.5.3",
      "eslint-config-prettier": ">=10.1.8",
      "form-data": ">=4.0.4",
      "synckit": ">=0.11.11",
      "@pkgr/core": ">=0.2.9"
    },
    "onlyBuiltDependencies": [
      "@bentley/imodeljs-native",
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "protobufjs"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^10.1.8"
  }
